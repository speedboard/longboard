{
    "name": "longboard",
    "version": "1.0.0-alpha.1",
    "description": "Electric Longboards",
    "main": "server.ts",
    "scripts": {
        "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha",
        "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | coveralls"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "exclude": [
            "**/*.js",
            "**/*.d.ts",
            "**/*.spec.ts"
        ],
        "reporter": [
            "html",
            "text"
        ],
        "all": true,
        "cache": false,
        "sourceMap": true,
        "instrument": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/speedboard/longboard.git"
    },
    "keywords": [
        "skate",
        "electric",
        "longboard"
    ],
    "author": "Ismael Queiroz <eu@iqueiroz.com.br>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/speedboard/longboard/issues"
    },
    "homepage": "https://speedboard.github.io",
    "dependencies": {
        "express": "^4.16.2",
        "typescript": "^2.6.2"
    },
    "devDependencies": {
        "@types/chai": "^4.1.0",
        "@types/expect": "^1.20.2",
        "@types/mocha": "^2.2.46",
        "@types/node": "^14.0.7",
        "@types/source-map": "^0.5.7",
        "chai": "^4.1.2",
        "coveralls": "^3.0.0",
        "istanbul": "^0.4.5",
        "mocha": "^4.1.0",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^11.4.1",
        "source-map-support": "^0.5.0",
        "ts-mocha": "^1.1.0",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "typescript": "^2.6.2",
        "typings": "^2.1.1"
    }
}
